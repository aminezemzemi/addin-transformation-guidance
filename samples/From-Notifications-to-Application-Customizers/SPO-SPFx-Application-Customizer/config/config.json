{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "status-bar-footer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/statusBarFooter/StatusBarFooterApplicationCustomizer.js",
          "manifest": "./src/extensions/statusBarFooter/StatusBarFooterApplicationCustomizer.manifest.json"
        }
      ]
    },
    "process-document-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/processDocument/ProcessDocumentCommandSet.js",
          "manifest": "./src/extensions/processDocument/ProcessDocumentCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "StatusBarFooterApplicationCustomizerStrings": "lib/extensions/statusBarFooter/loc/{locale}.js",
    "ProcessDocumentCommandSetStrings": "lib/extensions/processDocument/loc/{locale}.js"
  }
}
